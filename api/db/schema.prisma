//   email String  @unique
//   name  String?
// }


model UserProfile {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String
  about_me   String
  image_url String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
